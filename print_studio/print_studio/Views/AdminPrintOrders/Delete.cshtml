@model print_studio.Models.PrintOrder

@{
    ViewBag.Title = "Delete";
}

    <div class="container">
        <h2></h2>

        <h3>Подтвердите удаление заказа?</h3>
        <div>
            <h4>Номер заказа:  @Html.DisplayFor(model => model.phone)</h4>
            <hr />
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Подтвердить" class="btn btn-default" /> |
                    @Html.ActionLink("Отмена", "Index")
                </div>
            }
        </div>
    </div>